						"g_hclk_pe_arbi",		"g_aclk_peri_niu";

					rockchip,suspend-clkgating-setting=<0xf00f 0x0>;

					#clock-cells = <1>;
				};

				clk_gates10: gate-clk@00f8 {
					compatible = "rockchip,rk3188-gate-clk";
					reg = <0x00f8 0x4>;
					clocks =
						<&xin24m>,		<&xin24m>,
						<&xin24m>,		<&xin24m>,

						<&xin24m>,		<&xin24m>,
						<&xin24m>,		<&xin24m>,

