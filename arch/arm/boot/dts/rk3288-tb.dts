&adc {
	status = "okay";

	rockchip_headset {
		compatible = "rockchip_headset";
		headset_gpio = <&gpio0 GPIO_C2 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpio0_c2>;
		io-channels = <&adc 2>; 
