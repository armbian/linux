	compatible = "rockchip,mipi_dsi_init";
	rockchip,screen_init	= <1>;
	rockchip,dsi_lane	= <4>;
	rockchip,dsi_hs_clk	= <970>;
	rockchip,mipi_dsi_num	= <2>;
};

