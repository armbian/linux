# 3G device configuration
#

menuconfig 3G_MODULE
	tristate "3G module for mid"
	---help---
	  Say Y here if you have a support modem

choice
	depends on 3G_MODULE
	prompt  "Select 3G Module"

    config MU509
  	bool "MU509"

    config MT6229
    	bool "MT6229"

    config MW100
    	bool "MW100"

    config SEW868
	bool "SEW868"	

  


endchoice

